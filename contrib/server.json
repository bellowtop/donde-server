{
    "title": "this is a toml document",
    "loglevel": "debug",
    "server": {
        "name": "FaceDetectService"
    },
    "pipeline": {
        "detector": {
    	    "concurrent": 1,
	    "device_id": "CPU",
            "model": "./contrib/models/face-detection-adas-0001.xml",
	    "warmup": false
        },
	"landmarks": {
    	    "concurrent": 1,
	    "device_id": "CPU",
            "model": "./contrib/models/facial-landmarks-35-adas-0002.xml",
	    "warmup": false
        },
	"aligner": {
    	    "concurrent": 1,
	    "device_id": "CPU",
	    "warmup": false
        },
	"feature": {
